msgctxt "#30002"
msgid "Paths"
msgstr "Pfade"

msgctxt "#30003"
msgid "Local/Remote path, keys file selector"
msgstr "Lokaler/Entfernter Pfad, Schlüsseldatei-Wähler"

msgctxt "#30004"
msgid "Local"
msgstr "Lokal"

msgctxt "#30005"
msgid "Remote"
msgstr "Entfernt"

msgctxt "#30006"
msgid "Local Path (include Local Network)"
msgstr "Lokaler Pfad (inklusive lokalem Netzwerk)"

msgctxt "#30007"
msgid "Remote Path (Internet address)"
msgstr "Entfernter Pfad (Internet Adresse)"

msgctxt "#30008"
msgid "Path or Address haven't been set"
msgstr "Pfad oder Adresse wurde nicht gesetzt"

msgctxt "#30009"
msgid "Open settings window?"
msgstr "Einstellungsfenster öffnen?"

msgctxt "#30010"
msgid "Something's wrong!"
msgstr "Etwas stimmt nicht!"

msgctxt "#30011"
msgid "Incorrect settings or files detected"
msgstr "Inkorrekte Einstellungen oder Dateien erkannt"

msgctxt "#30012"
msgid "Youtube Setup addon is now ready to store your keys"
msgstr "Youtube Setup Addon ist nun bereit, Schlüssel zu speichern"

